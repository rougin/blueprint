{
    "name": "rougin/blueprint",
    "description": "Creates file-generating commands for your PHP applications",
    "keywords": [
        "blueprint",
        "command",
        "console",
        "file",
        "generator",
        "php"
        "template"
    ],
    "require": {
        "php": ">=5.3.0",
        "rdlowrey/auryn": "^1.0",
        "symfony/console": "^2.7",
        "symfony/yaml": "^2.7",
        "twig/twig": "^1.19"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "bin": [
        "bin/blueprint",
        "bin/blueprint.php"
    ],
    "autoload": {
        "psr-4": {
            "Rougin\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rougin\\Blueprint\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
