{
  "name": "rougin/blueprint",
  "description": "A bootstrap for your console projects in PHP.",
  "keywords": [ "blueprint", "generator", "file-generator" ],
  "homepage": "https://roug.in/blueprint/",
  "license": "MIT",
  "authors":
  [
    {
      "email": "rougingutib@gmail.com",
      "homepage": "https://roug.in/",
      "name": "Rougin Gutib",
      "role": "Software Engineer"
    }
  ],
  "require":
  {
    "php": ">=5.4.0",
    "league/flysystem": "~1.0",
    "rdlowrey/auryn": "~1.0",
    "rougin/slytherin": "~0.9",
    "symfony/console": "~2.8|~3.0|~4.0|~5.0|~6.0|~7.0",
    "symfony/yaml": "~2.8|~3.0|~4.0|~5.0|~6.0|~7.0",
    "twig/twig": "~1.24"
  },
  "require-dev":
  {
    "phpunit/phpunit": "~4.2|~5.7|~6.0|~7.0|~8.0|~9.0",
    "sanmai/phpunit-legacy-adapter": "~6.1|~8.0"
  },
  "bin":
  [
    "bin/blueprint",
    "bin/blueprint.php"
  ],
  "autoload":
  {
    "psr-4":
    {
      "Rougin\\Blueprint\\": "src"
    }
  },
  "autoload-dev":
  {
    "psr-4":
    {
      "Rougin\\Blueprint\\": "tests"
    }
  },
  "scripts":
  {
    "test": "phpunit"
  }
}