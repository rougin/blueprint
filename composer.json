{
    "name": "rougin/blueprint",
    "description": "Creates file-generating commands for your PHP applications",
    "keywords": [
        "blueprint",
        "command",
        "console",
        "file",
        "generator",
        "php",
        "template"
    ],
    "homepage": "https://github.com/rougin/blueprint",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "http://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "rdlowrey/auryn": "^1.0",
        "symfony/console": "~2.8",
        "symfony/yaml": "~2.8",
        "twig/twig": "^1.19"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "bin": [
        "bin/blueprint",
        "bin/blueprint.php"
    ],
    "autoload": {
        "psr-4": {
            "Rougin\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rougin\\Blueprint\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
