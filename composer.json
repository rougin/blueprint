{
    "name": "rougin/blueprint",
    "description": "A tool for generating files for your PHP projects",
    "keywords": [
        "blueprint",
        "template",
        "generator",
        "php"
    ],
    "require": {
        "php": ">=5.3.0",
        "twig/twig": "^1.19",
        "symfony/console": "^2.7",
        "symfony/yaml": "^2.7",
        "rdlowrey/auryn": "^1.0",
        "kevinlebrun/colors.php": "^0.4.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rougin\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rougin\\Blueprint\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
