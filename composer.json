{
    "name": "rougin/blueprint",
    "description": "Bootstraps your file-generating console applications.",
    "keywords": ["blueprint", "generator", "file-generator", "php"],
    "homepage": "https://rougin.github.io/blueprint/",
    "license": "MIT",
    "authors":
    [
        {
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io/",
            "name": "Rougin Gutib",
            "role": "Software Engineer"
        }
    ],
    "require":
    {
        "php": ">=5.4.0",
        "league/flysystem": "~1.0",
        "rdlowrey/auryn": "~1.0",
        "rougin/slytherin": "~0.9",
        "symfony/console": ">=2.8.0",
        "symfony/yaml": ">=2.8.0",
        "twig/twig": "~1.24"
    },
    "require-dev":
    {
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1"
    },
    "bin": ["bin/blueprint", "bin/blueprint.php"],
    "autoload":
    {
        "psr-4":
        {
            "Rougin\\Blueprint\\": "src"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Rougin\\Blueprint\\": "tests"
        }
    },
    "scripts":
    {
        "test": "phpunit"
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.0-dev"
        }
    }
}